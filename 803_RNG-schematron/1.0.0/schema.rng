   <define name="titleStmt">
      <element name="titleStmt">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(title statement) groups information about the title of a work and those responsible for its content. [2.2.1. The Title Statement 2.2. The File Description]</a:documentation>
         <group>
            <oneOrMore>
               <ref name="title"/>
            </oneOrMore>
            <optional>
               <ref name="author"/>
            </optional>
            <ref name="principal"/>
            <ref name="funder"/>
            <oneOrMore>
               <ref name="respStmt"/>
            </oneOrMore>
         </group>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="tibScholTexts-titleStmt-allRespsCovered-constraint-assert-8">
            <rule context="tei:titleStmt">
               <sch:assert xmlns="http://www.tei-c.org/ns/1.0"
                           xmlns:rng="http://relaxng.org/ns/structure/1.0"
                           xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                           xmlns:xi="http://www.w3.org/2001/XInclude"
                           test="exists(tei:respStmt[tei:resp = 'Transcribed by'])">a respStmt element with resp "Transcribed by" must be present in the title statment.</sch:assert>
               <sch:assert xmlns="http://www.tei-c.org/ns/1.0"
                           xmlns:rng="http://relaxng.org/ns/structure/1.0"
                           xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                           xmlns:xi="http://www.w3.org/2001/XInclude"
                           test="exists(tei:respStmt[tei:resp = 'TEI Markup by'])">a respStmt element with resp "TEI Markup by" must be present in the title statment.</sch:assert>
               <sch:assert xmlns="http://www.tei-c.org/ns/1.0"
                           xmlns:rng="http://relaxng.org/ns/structure/1.0"
                           xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                           xmlns:xi="http://www.w3.org/2001/XInclude"
                           test="exists(tei:respStmt[tei:resp = 'Data Modelling'])">a respStmt element with resp "Data Modelling" must be present in the title statment.</sch:assert>
               <sch:assert xmlns="http://www.tei-c.org/ns/1.0"
                           xmlns:rng="http://relaxng.org/ns/structure/1.0"
                           xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                           xmlns:xi="http://www.w3.org/2001/XInclude"
                           test="exists(tei:respStmt[tei:resp = 'APIS named entitites database implementation'])">a respStmt element with resp "APIS named entitites database implementation" must be present in the title statment.</sch:assert>
               <sch:assert xmlns="http://www.tei-c.org/ns/1.0"
                           xmlns:rng="http://relaxng.org/ns/structure/1.0"
                           xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                           xmlns:xi="http://www.w3.org/2001/XInclude"
                           test="exists(tei:respStmt[tei:resp = 'XML editor cumstomization'])">a respStmt element with resp "XML editor cumstomization" must be present in the title statment.</sch:assert>
               <sch:assert xmlns="http://www.tei-c.org/ns/1.0"
                           xmlns:rng="http://relaxng.org/ns/structure/1.0"
                           xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                           xmlns:xi="http://www.w3.org/2001/XInclude"
                           test="exists(tei:respStmt[tei:resp = 'ODD Implementation'])">a respStmt element with resp "ODD Implementation" must be present in the title statment.</sch:assert>
               <sch:assert xmlns="http://www.tei-c.org/ns/1.0"
